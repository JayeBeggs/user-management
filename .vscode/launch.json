{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Playwright: Debug signup_users",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@playwright/test/cli.js",
      "args": [
        "test",
        "--config",
        "e2e/web/playwright.config.ts",
        "e2e/web/tests/signup_users.spec.js"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "PWDEBUG": "1",
        "HEADLESS": "0"
      },
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": ["--inspect-brk"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "name": "Attach: Node 9229",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "restart": true,
      "timeout": 30000,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}

